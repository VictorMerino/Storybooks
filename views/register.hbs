<div class="section">
  Register
  <div class="row">
    <form method="POST" class="col s12">
      <div class="row">
        <div class="input-field col s6">
          <label for="firstName">First Name</label>
          <input placeholder="Placeholder" id="firstName" name="firstName" type="text" class="validate" value="{{this.firstName}}">
        </div>
        <div class="input-field col s6">
          <label for="lastName">Last Name</label>
          <input id="lastName" name="lastName" type="text" class="validate"  value="{{this.lastName}}">
        </div>
      </div>
      <div class="row">
        <div class="input-field col s12">
          <label for="password">Password</label>
          <input id="password" name="password" type="password" class="validate">
        </div>
      </div>
      <div class="row">
        <div class="input-field col s12">
          <label for="password2">Repeat Password</label>
          <input id="password2" name="password2" type="password" class="validate">
        </div>
      </div>
      <div class="row">
        <div class="input-field col s12">
          <label for="email">Email</label>
          <input id="email" name="email" type="email" class="validate"  value="{{this.email}}">
        </div>
      </div>
      <div class="row col s12">
        <button class="waves-effect waves-light btn-large" type="submit">Register</button>
      </div>
    </form>
  </div>
  <hr>
  Already have an account?
  So just <a href="/login">login</a>
</div>
